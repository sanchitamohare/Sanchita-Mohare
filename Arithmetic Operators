#include<stdio.h>
int main () {
 int a,b;
 printf("Enter a No. :");
 scanf("%d", &b);
 printf("Enter another No. :");
 scanf("%d", &b);
 printf("Addition is :%d\n", a+b);
 printf("Substraction is :%d\n", a-b);
 printf("Multiplication is :%d\n", a*b);
 printf("Division is :%d\n", a/b);
 printf("Modulo is :%d\n", a%b);
 return 0;
}
