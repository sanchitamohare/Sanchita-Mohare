#include<stdio.h>
struct student{
int roll;
float marks;
};
int main(){
struct student s[3];
int i;
for(i=0;i<3;i++){
printf("enter the roll and marks of student%d:",i+1);
scanf("%d %f",&s[i].roll, &s[i].marks);
}
for(i=0;i<3;i++){
printf(" The roll=%d marks=%f\n" ,s[i].roll,s[i].marks);
}

return 0;
}
